{
  "extends": "./.nuxt/tsconfig.json",
  "exclude": [
    "node_modules"
  ],
  "include": [
    "server/**/*.js",
    "server/**/*.ts",
    "**/*.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  }
}
